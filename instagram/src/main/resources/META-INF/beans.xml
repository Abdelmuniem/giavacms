	bean-discovery-mode="annotated">
	<interceptors>
		<class>org.giavacms.common.interceptor.LogOperationInterceptor</class>
	</interceptors>
	<scan>
		<exclude name="org.giavacms.instagram.api.model.aggregator.**" />
